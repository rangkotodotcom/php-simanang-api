{
    "name": "rangkotodotcom/simanang",
    "version": "1.0.1",
    "description": "An http client library to connect to the SIMANANG API",
    "type": "library",
    "keywords": [
        "laravel",
        "php",
        "simanang"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rangkotodotcom\\Simanang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rangkotodotcom\\Simanang\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jamilur Rusydi",
            "email": "softj4m98@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.3|^7.4|^8.0|^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.0.1|^7.2",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.25|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rangkotodotcom\\Simanang\\SimanangServiceProvider"
            ],
            "aliases": {
                "Simanang": "Rangkotodotcom\\Simanang\\SimanangFacade"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}